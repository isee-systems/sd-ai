{UTF-8}
Actual reproduction number = ZIDZ ( Infection , Infectious population )
	~	1/Week
	~		|

Additional case estimates = SMOOTH ( Asymptomatic infectious / Measurement period , Estimation period\
		 ) + SMOOTH ( Symptomatic infectious * ( 1 - Testing grade ) , Estimation period )
	~	Person/Week
	~		|

Asymptomatic incubation = Share of asymptomatic infections * Total incubation
	~	Person/Week
	~		|

Asymptomatic infectious = INTEG( Asymptomatic incubation - Recovery without symptoms \
		, 0)
	~	Person
	~		|

Average immunity period = 52
	~	Week
	~		|

Average length of symptomatic period = 1
	~	Week
	~		|

Average period of asymptomatic infections = 6
	~	Day
	~		|

"Average SARS-CoV-2 incubation time" = 3
	~	Day
	~		|

"Case fatality rate of COVID-19 patients" = 0.01
	~	Dmnl
	~		|

Cases tested = Symptomatic infectious * Testing grade
	~	Person/Week
	~		|

Chance of contact with an susceptible person = Susceptible population / Total population
	~	Dmnl
	~		|

Contacts per person per week = ( 1 - Social distancing measures ) * Normal number of contacts per week
	~	1/Week
	~		|

"Cumulative COVID-19 deaths" = INTEG( "Dying of COVID-19" , 0)
	~	Person
	~		|

Days per week = 7
	~	Day/Week
	~		|

Developing symptoms = DELAY3 ( Incubation + Influx of presymptomatic infectious people from abroad\
		 , Presymptomatic period / Days per week )
	~	Person/Week
	~		|

"Dying of COVID-19" = "Case fatality rate of COVID-19 patients" * Symptomatic infectious\
		 / Average length of symptomatic period
	~	Person/Week
	~		|

Estimated cumulative cases = INTEG( Additional case estimates + Cases tested , 0)
	~	Person
	~		|

Estimation period = WITH LOOKUP( Time , ([(0,0)-(104,20)],(0,20),(52,2),(104,1) ) )
	~	Week
	~		|

Exposed population = INTEG( Infection - Asymptomatic incubation - Incubation , 0)
	~	Person
	~		|

Incubation = ( 1 - Share of asymptomatic infections ) * Total incubation
	~	Person/Week
	~		|

Infection = Infectious population * Contacts per person per week * Infection rate per contact\
		 * Chance of contact with an susceptible person
	~	Person/Week
	~		|

Infection rate per contact = 0.3
	~	Dmnl
	~		|

Infectious population=
	INTEG ( Symptomatic infectious + Presymptomatic infectious + Asymptomatic infectious\
		 , 0 )
	~	Person
	~		|

Influx of presymptomatic infectious people from abroad = WITH LOOKUP( Time , ([(0,0)-\
		(104,2000)],(0,0),(4.5,70),(7,500),(8,2000),(10,1000),(12,80),(14,5),(17,5),(26,200\
		),(34,500),(35,200),(38,5),(52,5),(104,0) ) )
	~	Person/Week
	~		|

Initial Susceptible population = 1.7e+07
	~	Person
	~		|

Losing immunity = Recovered population / Average immunity period
	~	Person/Week
	~		|

Measurement period = 1
	~	Week
	~		|

Normal number of contacts per week = 7.5
	~	1/Week
	~		|

Presymptomatic infectious = INTEG( Incubation + Influx of presymptomatic infectious people from abroad\
		 - Developing symptoms , 0)
	~	Person
	~		|

Presymptomatic period = 3
	~	Day
	~		|

Recovered population = INTEG( Recovery + Recovery without symptoms - Losing immunity \
		, 0)
	~	Person
	~		|

Recovery = ( 1 - "Case fatality rate of COVID-19 patients" ) * Symptomatic infectious\
		 / Average length of symptomatic period
	~	Person/Week
	~		|

Recovery without symptoms=
	DELAY3 ( Asymptomatic infectious / ( Average period of asymptomatic infections / Days per week\
		 ) , Average period of asymptomatic infections / Days per week )
	~	Person/Week
	~		|

Share of asymptomatic infections = 0.5
	~	Dmnl
	~	Between 25 and 80%
	|

Social distancing measures= WITH LOOKUP (
	TIME STEP,
		 ([(0,0)-(104,1)],(0,0),(8.9,0),(9,0.1),(9.9,0.1),(10,0.4),(10.9,0.4),(11,0.7),(18.9\
		,0.7),(19,0.6),(21.9,0.6),(22,0.55),(40.9,0.5),(41,0.55),(49.9,0.55),(50,0.6),(55.9\
		,0.6),(56,0.7),(58,0.7),(104,0) ))
	~	Dmnl
	~		|

Susceptible population = INTEG( Losing immunity - Infection , Initial Susceptible population\
		 )
	~	Person
	~		|

Symptomatic infectious = INTEG( Developing symptoms - "Dying of COVID-19" - Recovery \
		, 0)
	~	Person
	~		|

Testing grade = WITH LOOKUP( Time , ([(0,0)-(104,0.9)],(0,0),(7,0),(10,0.2),(40,0.7),\
		(104,0.8) ) )
	~	1/Week
	~		|

Total incubation = DELAY3 ( Infection , ( "Average SARS-CoV-2 incubation time" / Days per week\
		 ) )
	~	Person/Week
	~		|

Total population = Susceptible population + Exposed population + Asymptomatic infectious\
		 + Presymptomatic infectious + Symptomatic infectious + Recovered population
	~	Person
	~		|

