	<model>
		<isee:extra_collection_variables>
			<isee:extra_collection_variable name="demand"/>
			<isee:extra_collection_variable name="desired_change_in_inventory"/>
			<isee:extra_collection_variable name="Inventory"/>
			<isee:extra_collection_variable name="producing"/>
		</isee:extra_collection_variables>
		<variables>
			<stock name="Inventory">
				<eqn>Desired_Inventory</eqn>
				<inflow>producing</inflow>
				<outflow>shipping</outflow>
				<non_negative/>
				<units>widgets</units>
			</stock>
			<flow name="producing">
				<eqn>Workers*productivity_per_worker</eqn>
				<non_negative/>
				<units>widget/mo</units>
			</flow>
			<stock name="Workers">
				<eqn>desired_workers</eqn>
				<inflow>hiring_or_firing</inflow>
				<non_negative/>
				<units>people</units>
			</stock>
			<flow name="shipping">
				<eqn>demand</eqn>
				<non_negative/>
				<units>widget/mo</units>
			</flow>
			<flow name="hiring or firing">
				<eqn>workers_gap/time_to_hire_or_fire</eqn>
				<units>person/mo</units>
			</flow>
			<aux name="productivity\nper worker">
				<eqn>2</eqn>
				<units>widgets per person per month</units>
			</aux>
			<aux name="Desired\nInventory">
				<eqn>100</eqn>
				<units>widgets</units>
			</aux>
			<aux name="inventory gap">
				<eqn>Desired_Inventory - Inventory</eqn>
				<units>widgets</units>
			</aux>
			<aux name="desired\nworkers">
				<eqn>desired_production/productivity_per_worker</eqn>
				<units>people</units>
			</aux>
			<aux name="time to\nhire or fire">
				<eqn>6</eqn>
				<units>months</units>
			</aux>
			<aux name="inventory\nadjustment time">
				<eqn>1</eqn>
				<units>months</units>
			</aux>
			<aux name="desired change\nin inventory">
				<eqn>inventory_gap/inventory_adjustment_time</eqn>
				<units>widget/mo</units>
			</aux>
			<aux name="demand">
				<eqn>TIME</eqn>
				<gf>
					<xscale min="0" max="3"/>
					<yscale min="0" max="100"/>
					<ypts>20,20,25,25</ypts>
				</gf>
				<units>widget/mo</units>
			</aux>
			<aux name="time to adjust\nexpectations">
				<eqn>6</eqn>
				<units>months</units>
			</aux>
			<aux name="desired\nproduction">
				<eqn>Expected_Demand + desired_change_in_inventory</eqn>
				<units>widget/mo</units>
			</aux>
			<aux name="workers gap">
				<eqn>desired_workers - Workers</eqn>
				<units>people</units>
			</aux>
			<stock name="Expected\nDemand">
				<eqn>demand</eqn>
				<inflow>changing_expected</inflow>
				<non_negative/>
				<units>widget/mo</units>
			</stock>
			<flow name="changing\nexpected">
				<eqn>(demand - Expected_Demand)/time_to_adjust_expectations</eqn>
				<units>widget/mo/Months</units>
			</flow>
		</variables>
