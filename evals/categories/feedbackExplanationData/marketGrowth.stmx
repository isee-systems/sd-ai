	<model_units/>
	<model>
		<isee:extra_collection_variables>
			<isee:extra_collection_variable name="Backlog"/>
			<isee:extra_collection_variable name="Sales_Force"/>
			<isee:extra_collection_variable name="Order_Rate"/>
			<isee:extra_collection_variable name="Shipment_Rate"/>
			<isee:extra_collection_variable name="Recent_Revenue"/>
		</isee:extra_collection_variables>
		<variables>
			<stock name="Backlog">
				<eqn>1</eqn>
				<inflow>Order_Rate</inflow>
				<outflow>Shipment_Rate</outflow>
				<non_negative/>
			</stock>
			<flow name="Order Rate">
				<eqn>Sales_Force*Sales_Effectiveness</eqn>
				<non_negative/>
			</flow>
			<stock name="Sales\nForce">
				<eqn>36.5</eqn>
				<inflow>Sales_Force_Net_Hiring_Rate</inflow>
				<non_negative/>
			</stock>
			<aux name="Normal Sales\nEffectiveness">
				<eqn>Reference_Sales_Effectiveness*Effect_Of_Market_Saturation_on_Sales_Effectiveness</eqn>
			</aux>
			<flow name="Sales Force Net Hiring Rate">
				<eqn>(Target_Sales_Force-Sales_Force)/Sales_Force_Adjustment_Time</eqn>
			</flow>
			<aux name="Target Sales Force">
				<eqn>Sales_Budget/Cost_Per_Sales_Representative</eqn>
			</aux>
			<aux name="Sales Force Adjustment Time">
				<eqn>18</eqn>
			</aux>
			<aux name="Sales Budget">
				<eqn>Recent_Revenue*Fraction_Revenue_To_Sales</eqn>
			</aux>
			<aux name="Cost Per Sales Representative">
				<eqn>8000</eqn>
			</aux>
			<aux name="Fraction Revenue To Sales">
				<eqn>0.2</eqn>
			</aux>
			<stock name="Recent\nRevenue">
				<eqn>Revenue</eqn>
				<inflow>Change_In_Recent_Revenue</inflow>
				<non_negative/>
			</stock>
			<flow name="Change In Recent Revenue">
				<eqn>(Revenue-Recent_Revenue)/Revenue_Reporting_Delay</eqn>
			</flow>
			<aux name="Revenue" flow_concept="true">
				<eqn>Shipment_Rate*Price</eqn>
			</aux>
			<aux name="Price">
				<eqn>10000</eqn>
			</aux>
			<flow name="Shipment Rate">
				<eqn>current_capacity*Capacity_Utilization</eqn>
				<non_negative/>
			</flow>
			<aux name="Revenue Reporting Delay">
				<eqn>3</eqn>
			</aux>
			<aux name="Desired Production">
				<eqn>Backlog/Company_Goal_for_Normal_Delivery_Delay</eqn>
			</aux>
			<aux name="Company Goal for Normal Delivery Delay">
				<eqn>2</eqn>
			</aux>
			<aux name="Bill To Book Ratio" flow_concept="true">
				<eqn>Shipment_Rate/(Order_Rate+0.00000000001)</eqn>
			</aux>
			<aux name="Effect Of Market Saturation on Sales Effectiveness">
				<eqn>MAX(1-Reference_Sales_Potential/Market_Sales_Potential,0)</eqn>
			</aux>
			<aux name="Market Sales\nPotential">
				<eqn>6000+0*8000</eqn>
			</aux>
			<aux name="Reference\nSales Effectiveness">
				<eqn>10</eqn>
			</aux>
			<aux name="Reference Sales Potential">
				<eqn>Sales_Force*Reference_Sales_Effectiveness</eqn>
			</aux>
			<aux name="Capacity Utilization">
				<eqn>Desired_Production/current_capacity</eqn>
				<gf>
					<xscale min="0" max="1.75"/>
					<yscale min="0" max="2"/>
					<ypts>0,0.35,0.61,0.83,1,1.15,1.23,1.25</ypts>
				</gf>
			</aux>
			<aux name="Initial Capacity">
				<eqn>500</eqn>
			</aux>
			<stock name="Recent\nCapacity">
				<eqn>Initial_Capacity</eqn>
				<inflow>Change_Recent_Capacity</inflow>
				<non_negative/>
			</stock>
			<flow name="Change Recent\nCapacity">
				<eqn>(current_capacity-Recent_Capacity)/0.5</eqn>
			</flow>
			<aux name="Effect Of Expansion Pressure On Desired Capacity">
				<eqn>Pressure_To_Expand_Capacity</eqn>
				<gf>
					<yscale min="0" max="2.5"/>
					<xpts>0,0.5,1,1.5,2,2.5,3,3.5,4,4.5,5</xpts>
					<ypts>0.3,0.75,1,1.2,1.4,1.6,1.8,2,2.1,2.17,2.2</ypts>
				</gf>
			</aux>
			<aux name="Pressure To Expand Capacity">
				<eqn>Delivery_Delay_Perceived_By_Company/Company_Goal_for_Normal_Delivery_Delay</eqn>
			</aux>
			<aux name="Delivery Delay">
				<eqn>Backlog/Recent_Shipment_Rate</eqn>
			</aux>
			<aux name="Time for Company To Perceive Delivery Delay">
				<eqn>3</eqn>
			</aux>
			<stock name="Recent\nShipment\nRate">
				<eqn>Backlog/Company_Goal_for_Normal_Delivery_Delay</eqn>
				<inflow>Change_Recent_Shipment_Rate</inflow>
				<non_negative/>
			</stock>
			<flow name="Change Recent\nShipment Rate">
				<eqn>(Shipment_Rate-Recent_Shipment_Rate)/0.5</eqn>
			</flow>
			<aux name="Sales Effectiveness">
				<eqn>Normal_Sales_Effectiveness*Effect_Of_Delivery_Delay_On_Sales_Effectiveness</eqn>
			</aux>
			<aux name="Effect Of Delivery Delay On Sales Effectiveness">
				<eqn>Delivery_Delay_Perceived_By_Market/Company_Goal_for_Normal_Delivery_Delay</eqn>
				<gf>
					<yscale min="0" max="1.1"/>
					<xpts>0,0.5,1,1.5,2,2.5,3,3.5,4,4.5,5</xpts>
					<ypts>1.1,1.07,1,0.9,0.7,0.4,0.2,0.1,0.05,0.02,0</ypts>
				</gf>
			</aux>
			<aux name="Time For Market To\nPerceive Delivery Delay Over and\nBeyond the Time for the Company to Perceive the Delivery Delay">
				<eqn>12</eqn>
			</aux>
			<stock name="Delivery Delay\nPerceived By Market">
				<eqn>Delivery_Delay_Perceived_By_Company</eqn>
				<inflow>delivery_delay_perceived_by_market_change</inflow>
				<non_negative/>
			</stock>
			<flow name="delivery delay\nperceived by market\nchange">
				<eqn>(Delivery_Delay_Perceived_By_Company-Delivery_Delay_Perceived_By_Market)/Time_For_Market_To_Perceive_Delivery_Delay_Over_and_Beyond_the_Time_for_the_Company_to_Perceive_the_Delivery_Delay</eqn>
			</flow>
			<stock name="Delivery Delay\nPerceived By Company">
				<eqn>Delivery_Delay</eqn>
				<inflow>delivery_delay_perceived_by_company_change</inflow>
				<non_negative/>
			</stock>
			<flow name="delivery delay\nperceived by company\nchange">
				<eqn>(Delivery_Delay-Delivery_Delay_Perceived_By_Company)/Time_for_Company_To_Perceive_Delivery_Delay</eqn>
			</flow>
			<aux name="current capacity">
				<eqn>DELAY3(desired_capacity,Capacity_Acquisition_Delay,  Initial_Capacity)</eqn>
				<isee:delay_aux/>
			</aux>
			<aux name="Capacity Acquisition Delay">
				<eqn>72</eqn>
			</aux>
			<aux name="desired capacity">
				<eqn>Effect_Of_Expansion_Pressure_On_Desired_Capacity*Recent_Capacity</eqn>
			</aux>
		</variables>
